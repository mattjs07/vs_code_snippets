				"\\DeclareMathOperator{\\E}{\\mathbb{E}}\r",
				"\\DeclareMathOperator{\\V}{V}\r",
				"\\DeclareMathOperator{\\Var}{Var}\r",
				"\\DeclareMathOperator{\\Cov}{Cov}\r",
